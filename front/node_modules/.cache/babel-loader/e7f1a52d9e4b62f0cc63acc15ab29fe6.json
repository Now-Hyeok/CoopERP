{"ast":null,"code":"import axios from 'axios';\nimport { mapActions, mapMutations, mapState } from 'vuex';\nimport shipmentRegister from '@/components/ShipmentRegister.vue';\nexport default {\n  name: 'transactionShipment',\n  computed: { ...mapState(['shipmentList', 'quantityList', 'shipModal'])\n  },\n  methods: { ...mapMutations(['openShipModal', 'closeShipModal']),\n    ...mapActions(['getShipmentList', 'getQuantity']),\n\n    deleteShipment(id) {\n      axios.delete(`/api/shipment/delete/${id}`).then(() => {\n        this.getShipmentList();\n        this.getQuantity();\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n\n  },\n  components: {\n    shipmentRegister\n  }\n};","map":{"version":3,"mappings":"AAmEA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,UAAT,EAAqBC,YAArB,EAAmCC,QAAnC,QAAmD,MAAnD;AACA,OAAOC,gBAAP,MAA6B,mCAA7B;AAEA,eAAe;EACbC,IAAI,EAAE,qBADO;EAEbC,QAAQ,EAAE,EACR,GAAGH,QAAQ,CAAC,CAAC,cAAD,EAAgB,cAAhB,EAA+B,WAA/B,CAAD;EADH,CAFG;EAKbI,OAAO,EAAE,EACP,GAAGL,YAAY,CAAC,CAAC,eAAD,EAAiB,gBAAjB,CAAD,CADR;IAEP,GAAGD,UAAU,CAAC,CAAC,iBAAD,EAAmB,aAAnB,CAAD,CAFN;;IAGPO,cAAc,CAACC,EAAD,EAAI;MAChBT,KAAK,CAACU,MAAN,CAAc,wBAAuBD,EAAG,EAAxC,EACCE,IADD,CACM,MAAI;QACR,KAAKC,eAAL;QACA,KAAKC,WAAL;MACD,CAJD,EAKCC,KALD,CAKQC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CAPD;IAQD;;EAZM,CALI;EAmBbG,UAAU,EAAC;IAACd;EAAD;AAnBE,CAAf","names":["axios","mapActions","mapMutations","mapState","shipmentRegister","name","computed","methods","deleteShipment","id","delete","then","getShipmentList","getQuantity","catch","err","console","error","components"],"sourceRoot":"","sources":["D:\\project\\CoopERP\\front\\src\\pages\\TransactionManagement\\TransactionShipment.vue"],"sourcesContent":["<template>\r\n\r\n<shipmentRegister v-if=\"shipModal==true\" @shipRegister=\"closeShipModal();getShipmentList();getQuantity();\"/>\r\n\r\n<div class=\"register\">\r\n  <div class=\"received-list\">\r\n    <table class=\"inven-table\">\r\n    <caption>Shipment</caption>\r\n    <thead >\r\n      <tr>\r\n        <th scope=\"col\">#</th>\r\n        <th scope=\"col\">Shipment Date</th>\r\n        <th scope=\"col\">Buyer</th>\r\n        <th scope=\"col\">Product</th>\r\n        <th scope=\"col\">Quantity</th>\r\n        <th scope=\"col\">Sales Price</th>\r\n        <th scope=\"col\">Delete</th>\r\n      </tr>\r\n    </thead>\r\n    <tbody>\r\n      <tr class=\"shipmentList\" scope=\"row\" v-for=\"(item,i) in shipmentList\" :key=\"item\" >\r\n        <th scope=\"row\">{{i+1}}</th>\r\n        <td>{{item.Shipment_date.substr(0,10)}}</td>\r\n        <td>{{item.Shipment_buyer}}</td>\r\n        <td>{{item.Product_name}}</td>\r\n        <td>{{item.Shipment_amount}}</td>\r\n        <td>{{item.Shipment_price}}</td>\r\n        <td><button class=\"btn btn-light\" @click=\"deleteShipment(item.Shipment_id)\">x</button></td>\r\n\r\n      </tr>\r\n    </tbody>\r\n\r\n  </table>\r\n\r\n  </div>\r\n  \r\n  <div class=\"inventory\">\r\n    <table class=\"inven\">\r\n      <caption>Total Quantity</caption>\r\n      <thead >\r\n        <tr>\r\n          <th scope=\"col\">#</th>\r\n          <th scope=\"col\">Product</th>\r\n          <th scope=\"col\">Current Quantity</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr scope=\"row\" v-for=\"(item,i) in quantityList\" :key=\"item\">\r\n          <th>{{i+1}}</th>\r\n          <td>{{item.Product_name}}</td>\r\n          <td>{{`${item.Warehousing_amount} ${item.Unit}`}}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n    <div class=\"inven-btn\">\r\n      <button type=\"button\" class=\"btn btn-primary\" @click=\"openShipModal()\">\r\n    New Shipment\r\n    </button> \r\n    </div>\r\n    \r\n  </div>\r\n</div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { mapActions, mapMutations, mapState } from 'vuex'\r\nimport shipmentRegister from '@/components/ShipmentRegister.vue'\r\n\r\nexport default {\r\n  name: 'transactionShipment',\r\n  computed: {\r\n    ...mapState(['shipmentList','quantityList','shipModal']),\r\n  },\r\n  methods: {\r\n    ...mapMutations(['openShipModal','closeShipModal']),\r\n    ...mapActions(['getShipmentList','getQuantity']),\r\n    deleteShipment(id){\r\n      axios.delete(`/api/shipment/delete/${id}`)\r\n      .then(()=>{\r\n        this.getShipmentList();\r\n        this.getQuantity();\r\n      })\r\n      .catch((err)=>{\r\n        console.error(err);\r\n      })\r\n    },\r\n  },\r\n  components:{shipmentRegister}\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.register{\r\n  padding:20px;\r\n  height: 100%;\r\n  width: 100%;\r\n  overflow: auto;\r\n}\r\n\r\n.ship{\r\n  float: left;\r\n  width:50%;\r\n  height: 100%;\r\n}\r\n.quantity{\r\n  float: left;\r\n  width:50%;\r\n  height: 100%;\r\n}\r\n\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}