{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { mapMutations, mapState } from 'vuex';\nexport default {\n  name: \"memberRegister\",\n  computed: { ...mapState(['memberModal', 'user'])\n  },\n\n  data() {\n    return {\n      errors: [],\n      name: null,\n      id: null,\n      address: null,\n      phone: null,\n      password: null,\n      area: null\n    };\n  },\n\n  methods: { ...mapMutations(['closeMemberModal']),\n\n    sendMemberData() {\n      axios.post('/api/member/registration', {\n        name: this.name,\n        phone: this.phone,\n        password: this.password,\n        address: this.address,\n        coop: this.user.Coop_id,\n        area: this.area\n      }).then(response => {\n        this.$emit('memberRegister');\n        console.log(response);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n\n    registerMember() {\n      if (this.name && this.address && this.phone && this.password) {\n        this.sendMemberData();\n        return true;\n      }\n\n      this.errors = [];\n\n      if (!this.name) {\n        this.errors.push('Name required\\n');\n      }\n\n      if (!this.address) {\n        this.errors.push('address require\\n');\n      } // if(!this.phone){\n      //   this.errors.push('id require');\n      // }\n\n\n      if (!this.password) {\n        this.errors.push('password require');\n      }\n\n      alert(this.errors);\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA+CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,YAAT,EAAuBC,QAAvB,QAAuC,MAAvC;AAGA,eAAe;EACbC,IAAI,EAAC,gBADQ;EAEbC,QAAQ,EAAC,EACL,GAAGF,QAAQ,CAAC,CAAC,aAAD,EAAe,MAAf,CAAD;EADN,CAFI;;EAMbG,IAAI,GAAE;IACJ,OAAM;MACJC,MAAM,EAAC,EADH;MAEJH,IAAI,EAAC,IAFD;MAGJI,EAAE,EAAC,IAHC;MAIJC,OAAO,EAAC,IAJJ;MAKJC,KAAK,EAAC,IALF;MAMJC,QAAQ,EAAC,IANL;MAOJC,IAAI,EAAC;IAPD,CAAN;EASD,CAhBY;;EAkBbC,OAAO,EAAC,EACN,GAAGX,YAAY,CAAC,CAAC,kBAAD,CAAD,CADT;;IAGNY,cAAc,GAAE;MACdb,KAAK,CAACc,IAAN,CAAW,0BAAX,EAAsC;QACpCX,IAAI,