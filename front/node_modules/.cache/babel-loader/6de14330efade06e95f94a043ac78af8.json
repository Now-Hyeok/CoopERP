{"ast":null,"code":"import { mapActions, mapMutations, mapState } from 'vuex';\nimport InventoryRegister from '@/components/inventoryRegister.vue';\nimport axios from 'axios';\nexport default {\n  name: 'inventoryStatus',\n  components: {\n    InventoryRegister\n  },\n  computed: { ...mapState(['receivedList', 'inventoryModal', 'quantityList'])\n  },\n  methods: { ...mapMutations(['openInventoryModal', 'closeInventoryModal']),\n    ...mapActions(['getReceived', 'productList', 'getQuantity']),\n\n    getData() {\n      this.getQuantity();\n      this.getReceived();\n    },\n\n    deleteInventory(id) {\n      axios.delete(`/api/inventory/delete/${id}`).then(() => {\n        this.getData();\n      }).catch(err => {\n        console.error(err);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgEA,SAAUA,UAAV,EAAsBC,YAAtB,EAAoCC,QAApC,QAAoD,MAApD;AACA,OAAOC,iBAAP,MAA8B,oCAA9B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAc;EACVC,IAAI,EAAC,iBADK;EAEVC,UAAU,EAAC;IAAEH;EAAF,CAFD;EAGVI,QAAQ,EAAC,EACL,GAAGL,QAAQ,CAAC,CAAC,cAAD,EAAgB,gBAAhB,EAAiC,cAAjC,CAAD;EADN,CAHC;EAOVM,OAAO,EAAC,EACJ,GAAGP,YAAY,CAAC,CAAC,oBAAD,EAAsB,qBAAtB,CAAD,CADX;IAEJ,GAAGD,UAAU,CAAC,CAAC,aAAD,EAAe,aAAf,EAA6B,aAA7B,CAAD,CAFT;;IAIJS,OAAO,GAAE;MACL,KAAKC,WAAL;MACA,KAAKC,WAAL;IACH,CAPG;;IAQJC,eAAe,CAACC,EAAD,EAAI;MACfT,KAAK,CAACU,MAAN,CAAc,yBAAwBD,EAAG,EAAzC,EACCE,IADD,CACM,MAAI;QACN,KAAKN,OAAL;MACH,CAHD,EAICO,KAJD,CAIQC,GAAD,IAAO;QACVC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACH,CAND;IAQJ;;EAjBI;AAPE,CAAd","names":["mapActions","mapMutations","mapState","InventoryRegister","axios","name","components","computed","methods","getData","getQuantity","getReceived","deleteInventory","id","delete","then","catch","err","console","error"],"sourceRoot":"","sources":["D:\\project\\CoopERP\\front\\src\\pages\\InventoryManagement\\InventoryStatus.vue"],"sourcesContent":["<template>\r\n\r\n\r\n<InventoryRegister v-if=\"inventoryModal == true\" @inventoryRegister=\"getData();closeInventoryModal();\"/>\r\n\r\n<div class=\"received-list\">\r\n    <table class=\"inven-table\">\r\n        <caption>received list</caption>\r\n        <thead>\r\n            <th scope=\"col\">#</th>\r\n            <th scope=\"col\">Product</th>\r\n            <th scope=\"col\">Quantity</th>\r\n            <th scope=\"col\">Member name</th>\r\n            <th scope=\"col\">Price</th>\r\n            <th scope=\"col\">Quailty</th>\r\n            <th scope=\"col\">Receiving date</th>\r\n            <th scope=\"col\">delete</th>\r\n        </thead>\r\n        <tbody>\r\n            <tr scope=\"row\" class=\"inventoryList\" v-for=\"(item,i) in receivedList\" :key=\"item\">\r\n                <th scope=\"row\">{{i+1}}</th>\r\n                <td>{{item.Product_name}}</td>\r\n                <td>{{item.Warehousing_amount}}</td>\r\n                <td>{{item.Member_name}}</td>\r\n                <td>{{item.Price}}</td>\r\n                <td>{{item.Product_quailty}}</td>\r\n                <td>{{item.Warehousing_date.substr(0,10)}}</td>\r\n                <td><button type=\"button\" class=\"btn btn-light\" @click=\"deleteInventory(item.Warehousing_id)\">X</button></td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <div class=\"empty-box\">\r\n\r\n    </div>\r\n</div>\r\n\r\n<div class=\"inventory\">\r\n    <!-- 입고예정 수량 + 입고된 수량 -->\r\n    <table class=\"inven\">\r\n        <caption>inventory</caption>\r\n        <thead>\r\n            <th scope=\"col\">#</th>\r\n            <th scope=\"col\">Product</th>\r\n            <th scope=\"col\">Current Quantity</th>\r\n            <th scope=\"col\">warehousing schedule Quantity</th>\r\n        </thead>\r\n        <tbody>\r\n            <tr scope=\"row\" v-for=\"(item,i) in quantityList\" :key=\"item\">\r\n                <th>{{i+1}}</th>\r\n                <td>{{item.Product_name}}</td>\r\n                <td>{{`${item.Warehousing_amount} ${item.Unit}`}}</td>\r\n                <td>{{`${item.Shipment_amount} ${item.Unit}`}}</td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <div class=\"inven-btn\">\r\n        <button class=\"btn btn-primary\" @click=\"openInventoryModal()\">inventory Register</button>\r\n    </div>\r\n</div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport {  mapActions, mapMutations, mapState } from 'vuex'\r\nimport InventoryRegister from '@/components/inventoryRegister.vue'\r\nimport axios from 'axios'\r\nexport default{\r\n    name:'inventoryStatus',\r\n    components:{ InventoryRegister },\r\n    computed:{\r\n        ...mapState(['receivedList','inventoryModal','quantityList']),\r\n    },\r\n\r\n    methods:{\r\n        ...mapMutations(['openInventoryModal','closeInventoryModal']),\r\n        ...mapActions(['getReceived','productList','getQuantity']),\r\n        \r\n        getData(){\r\n            this.getQuantity();\r\n            this.getReceived();\r\n        }, \r\n        deleteInventory(id){\r\n            axios.delete(`/api/inventory/delete/${id}`)\r\n            .then(()=>{\r\n                this.getData();\r\n            })\r\n            .catch((err)=>{\r\n                console.error(err)\r\n            })\r\n\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.inven{\r\n    text-align: center;\r\n    font-size: 12px;\r\n    width:400px\r\n}\r\n.inven-table{\r\n    text-align: center;\r\n    font-size: 12px;\r\n    margin:auto;\r\n    width:600px;\r\n}\r\n.inventory{\r\n    float: left;\r\n    height: 100%;\r\n    width:40%\r\n}\r\n.received-list{\r\n    float: left;\r\n    height: 100%;\r\n    width:60%;\r\n}\r\n\r\n.inven-btn{\r\n    margin-top:20px;\r\n    text-align: right;\r\n    margin-right: 120px;\r\n}\r\n.empty-box{\r\n    height: 50px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}