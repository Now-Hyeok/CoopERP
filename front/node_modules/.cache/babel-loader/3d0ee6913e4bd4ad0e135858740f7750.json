 \r\n  <button class=\"btn btn-primary\" @click=\"openPostModal()\">new post</button>\r\n</div>\r\n\r\n  \r\n  \r\n</template>\r\n\r\n<script>\r\nimport WritePost from '@/components/WritePost.vue'\r\n\r\nimport { mapActions, mapMutations, mapState } from 'vuex'\r\n\r\nexport default {\r\n  components: { WritePost },\r\n  created(){\r\n    this.setCommentList({})\r\n  },\r\n    computed:{\r\n        ...mapState(['postList','postModal'])\r\n    },\r\n    methods:{\r\n        ...mapMutations(['openPostModal','closePostModal']),\r\n        ...mapActions(['getPostList',]),\r\n        ...mapMutations(['setCommentList'])\r\n    }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.post-table{\r\n  margin:auto;\r\n  width:1000px;\r\n  font-size:10px;\r\n  margin-top: 8px;\r\n  text-align: center;\r\n}\r\n\r\n.post-num{\r\n  width: 10px;\r\n}\r\n.post-text{\r\n  width:600px;\r\n}\r\n\r\n.post-lost{\r\n  height: 2rem;\r\n}\r\n.postbtn{\r\n  text-align: right;\r\n  margin-right: 150px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}