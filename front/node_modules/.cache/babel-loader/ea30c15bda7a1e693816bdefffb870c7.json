{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { mapMutations, mapState } from 'vuex';\nexport default {\n  name: \"shipRegister\",\n  computed: { ...mapState(['shipModal', 'productList'])\n  },\n\n  data() {\n    return {\n      errors: [],\n      product: null,\n      date: null,\n      amount: null,\n      price: null,\n      buyer: null\n    };\n  },\n\n  methods: { ...mapMutations(['closeShipModal']),\n\n    sendShipData() {\n      axios.post('/api/shipment/registration', {\n        product: this.product,\n        date: this.date,\n        amount: this.amount,\n        price: this.price,\n        buyer: this.buyer\n      }).then(() => {\n        this.$emit('shipRegister');\n      }).catch(err => {\n        console.error(err);\n      });\n    },\n\n    registerShip() {\n      if (this.product && this.date && this.amount && this.price && this.buyer) {\n        this.sendShipData();\n        return true;\n      }\n\n      this.errors = [];\n\n      if (!this.product) {\n        this.errors.push('Product required');\n      }\n\n      if (!this.date) {\n        this.errors.push('date require');\n      }\n\n      if (!this.amount) {\n        this.errors.push('amount require');\n      }\n\n      if (!this.price) {\n        this.errors.push('price require');\n      }\n\n      if (!this.buyer) {\n        this.errors.push('buyer require');\n      }\n\n      alert(this.errors);\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAgDQ,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,YAAT,EAAuBC,QAAvB,QAAuC,MAAvC;AAGA,eAAe;EACbC,IAAI,EAAC,cADQ;EAEbC,QAAQ,EAAC,EACL,GAAGF,QAAQ,CAAC,CAAC,WAAD,EAAa,aAAb,CAAD;EADN,CAFI;;EAMbG,IAAI,GAAE;IACJ,OAAM;MACJC,MAAM,EAAC,EADH;MAEJC,OAAO,EAAC,IAFJ;MAGJC,IAAI,EAAC,IAHD;MAIJC,MAAM,EAAC,IAJH;MAKJC,KAAK,EAAC,IALF;MAMJC,KAAK,EAAC;IANF,CAAN;EAQD,CAfY;;EAiBbC,OAAO,EAAC,EACN,GAAGX,YAAY,CAAC,CAAC,gBAAD,CAAD,CADT;;IAGNY,YAAY,GAAE;MACZb,KAAK,CAACc,IAAN,CAAW,4BAAX,EAAwC;QACtCP,OAAO,EAAE,KAAKA,OADwB;QAEtCC,IAAI,EAA