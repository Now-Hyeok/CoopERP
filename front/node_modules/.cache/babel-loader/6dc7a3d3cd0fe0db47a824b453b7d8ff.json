{"ast":null,"code":"import { mapActions, mapMutations, mapState } from 'vuex';\nimport WarehousingRegister from '@/components/WarehousingRegister.vue';\nimport axios from 'axios';\nexport default {\n  name: \"inventoryStatus\",\n  computed: { ...mapState(['warehousingModal', 'warehousingList', 'memberList', 'productList'])\n  },\n  methods: { ...mapMutations([\"openWarehousingModal\", \"closeWarehousingModal\"]),\n    ...mapActions(['getWarehousing', 'getReceived', 'getQuantity', 'getManage']),\n\n    deleteWarehousing(id) {\n      axios.delete(`/api/warehousing/delete/${id}`).then(() => {\n        this.getWarehousing();\n        this.getQuantity();\n        this.getManage();\n      }).catch(err => {\n        console.error(err);\n      });\n    },\n\n    received(id) {\n      axios.get(`/api/warehousing/received/${id}`).then(() => {\n        this.deleteWarehousing(id);\n        this.getReceived();\n        this.getManage();\n      }).catch(err => {\n        console.error(err);\n      }); //warehousing schedule 들어가서 received 를 true 로 바꾸는코드\n    }\n\n  },\n  components: {\n    WarehousingRegister\n  }\n};","map":{"version":3,"mappings":"AA4CA,SAASA,UAAT,EAAqBC,YAArB,EAAmCC,QAAnC,QAAmD,MAAnD;AACA,OAAOC,mBAAP,MAAgC,sCAAhC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACXC,IAAI,EAAE,iBADK;EAEXC,QAAQ,EAAC,EACP,GAAGJ,QAAQ,CAAC,CAAC,kBAAD,EAAoB,iBAApB,EAAsC,YAAtC,EAAmD,aAAnD,CAAD;EADJ,CAFE;EAKXK,OAAO,EAAE,EACL,GAAGN,YAAY,CAAC,CAAC,sBAAD,EAAwB,uBAAxB,CAAD,CADV;IAEL,GAAGD,UAAU,CAAC,CAAC,gBAAD,EAAkB,aAAlB,EAAgC,aAAhC,EAA8C,WAA9C,CAAD,CAFR;;IAILQ,iBAAiB,CAACC,EAAD,EAAI;MACrBL,KAAK,CAACM,MAAN,CAAc,2BAA0BD,EAAG,EAA3C,EACCE,IADD,CACM,MAAI;QACR,KAAKC,cAAL;QACA,KAAKC,WAAL;QACA,KAAKC,SAAL;MACD,CALD,EAMCC,KAND,CAMQC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CARD;IASD,CAdM;;IAePG,QAAQ,CAACV,EAAD,EAAI;MACVL,KAAK,CAACgB,GAAN,CAAW,6BAA4BX,EAAG,EAA1C,EACCE,IADD,CACM,MAAI;QACR,KAAKH,iBAAL,CAAuBC,EAAvB;QACA,KAAKY,WAAL;QACA,KAAKP,SAAL;MACD,CALD,EAMCC,KAND,CAMQC,GAAD,IAAO;QACZC,OAAO,CAACC,KAAR,CAAcF,GAAd;MACD,CARD,EADU,CAUV;IACF;;EA1BO,CALE;EAiCXM,UAAU,EAAE;IAAEnB;EAAF;AAjCD,CAAf","names":["mapActions","mapMutations","mapState","WarehousingRegister","axios","name","computed","methods","deleteWarehousing","id","delete","then","getWarehousing","getQuantity","getManage","catch","err","console","error","received","get","getReceived","components"],"sourceRoot":"","sources":["D:\\project\\CoopERP\\front\\src\\pages\\InventoryManagement\\InventoryWarehousing.vue"],"sourcesContent":["<template>\r\n<WarehousingRegister v-if=\"warehousingModal == true\" @warehousingRegister=\"closeWarehousingModal(); getWarehousing();getQuantity(); getManage();\"/>\r\n\r\n<div>\r\n  <table class=\"user-table\">\r\n    <caption>Warehousing schedule</caption>\r\n    <thead >\r\n      <tr>\r\n        <th scope=\"col\">#</th>        \r\n        <th scope=\"col\">Product</th>\r\n        <th scope=\"col\">quantity</th>\r\n        <th scope=\"col\">member name</th>\r\n        <th scope=\"col\">request price</th>\r\n        <th scope=\"col\">schedule</th>\r\n        <th scope=\"col\">quailty</th>\r\n        <th scope=\"col\">warehousing</th>\r\n        <th scope=\"col\">delete</th>\r\n      </tr>\r\n    </thead>\r\n    <tbody>\r\n      <tr class=\"warehousingList\" scope=\"row\" v-for=\"(item,i) in warehousingList\" :key=\"item\">\r\n        <th scope=\"row\">{{i+1}}</th>\r\n        <td>{{item.Product_name}}</td>\r\n        <td>{{item.Shipment_amount}}</td>\r\n        <td>{{item.Member_name}}</td>\r\n        <td>{{item.Req_price}}</td>\r\n        <td>{{item.Shipment_date.substr(0,10)}}</td>\r\n        <td>{{item.Product_quailty}}</td>\r\n        <td><button @click=\"received(item.Schedule_id)\" type=\"button\" class=\"btn btn-light\">Received</button></td>\r\n        <td><button type=\"button\" class=\"btn btn-light\" @click=\"deleteWarehousing(item.Schedule_id)\">X</button></td>\r\n      </tr>\r\n    </tbody>\r\n  </table>\r\n\r\n  <div class=\"btBtn\">\r\n    <button type=\"button\" class=\"btn btn-primary\" @click=\"openWarehousingModal()\">\r\n    New Warehousing\r\n    </button>\r\n  </div>\r\n</div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapMutations, mapState } from 'vuex'\r\nimport WarehousingRegister from '@/components/WarehousingRegister.vue'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: \"inventoryStatus\",\r\n    computed:{\r\n      ...mapState(['warehousingModal','warehousingList','memberList','productList'])\r\n    },\r\n    methods: {\r\n        ...mapMutations([\"openWarehousingModal\",\"closeWarehousingModal\"]),\r\n        ...mapActions(['getWarehousing','getReceived','getQuantity','getManage']),\r\n\r\n        deleteWarehousing(id){\r\n        axios.delete(`/api/warehousing/delete/${id}`)\r\n        .then(()=>{\r\n          this.getWarehousing();\r\n          this.getQuantity();\r\n          this.getManage();\r\n        })\r\n        .catch((err)=>{\r\n          console.error(err);\r\n        })\r\n      },\r\n      received(id){\r\n        axios.get(`/api/warehousing/received/${id}`)\r\n        .then(()=>{\r\n          this.deleteWarehousing(id);\r\n          this.getReceived();\r\n          this.getManage();\r\n        })\r\n        .catch((err)=>{\r\n          console.error(err);\r\n        })\r\n        //warehousing schedule 들어가서 received 를 true 로 바꾸는코드\r\n      }\r\n    },\r\n    components: { WarehousingRegister }\r\n    ,\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}